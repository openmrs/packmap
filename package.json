{
  "name": "packmap",
  "version": "1.2.0",
  "description": "Turn a package.json into a browser-ready import map",
  "main": "lib/packmap.js",
  "bin": "bin/packmap-cli.js",
  "scripts": {
    "check-format": "prettier 'lib/**/*' 'bin/**/*' '__mocks__/**/*' 'test/**/*' --check",
    "test": "jest --config jest.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmrs/packmap.git"
  },
  "keywords": [
    "import",
    "map",
    "esm",
    "es",
    "module",
    "package"
  ],
  "files": [
    "bin",
    "lib"
  ],
  "author": "Joel Denning",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/openmrs/packmap/issues"
  },
  "homepage": "https://github.com/openmrs/packmap#readme",
  "dependencies": {
    "commander": "^3.0.2",
    "fs-extra": "^8.1.0",
    "sort-object-keys": "^1.1.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "husky": "^3.0.7",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1"
  }
}
